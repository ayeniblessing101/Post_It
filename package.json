  "name": "postit",
  "version": "1.0.0",
  "description": "A simple application that allows friends and colleagues create groups for notifications. ",
  "main": "./app/client/index.js",
  "scripts": {
    "start": "nodemon --watch app/server --exec babel-node -- app/server/app.js",
    "server:test": "NODE_ENV=test DATABASE_URL=postgres://ytettkxo:BL2lc8F27dwSmxQkcVwhjPXW2gUJZo8m@stampy.db.elephantsql.com:5432/ytettkxo mocha --timeout 90000  ./app/server/test/ --compilers js:babel-register",
    "client:test": "jest app/client/ --coverage",
    "test-with-coverage": "NODE_ENV=test DATABASE_URL=postgres://ytettkxo:BL2lc8F27dwSmxQkcVwhjPXW2gUJZo8m@stampy.db.elephantsql.com:5432/ytettkxo nyc --reporter=lcov --reporter=text mocha --timeout 90000 ./app/server/test --compilers js:babel-register",
